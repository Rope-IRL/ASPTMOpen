@model IList<(FlatsContract, Flat)>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    <div>
        @foreach (var item in Model)
        {
            <div class="contract">
                <div class="contract-flat-header">
                    @item.Item2.Header
                </div>
                <div class="contract-reservation_dates">
                    <div class="contract-reservation_dates-start">
                       Start date of reservation: @item.Item1.StartDate
                    </div>
                    <div class="contract-reservation_dates-end">
                        End date of reservation: @item.Item1.EndDate
                    </div>
                </div>
                <div class="contract-cost">
                    Cost is @item.Item1.Cost
                </div>
            </div>
        }
    </div>
}
