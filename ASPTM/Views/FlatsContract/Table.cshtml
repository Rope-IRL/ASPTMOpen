@using ASPTM.ViewModels
@model IEnumerable<FlatsContractInfo>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    <div>
        @foreach (var item in Model)
        {
            <div class="contract">
                <div class="contract-flat-header">
                    @item.FlatAddress
                </div>
                <div class="contract-reservation_dates">
                    <div class="contract-reservation_dates-start">
                       Start date of reservation: @item.StartDate
                    </div>
                    <div class="contract-reservation_dates-end">
                        End date of reservation: @item.EndDate
                    </div>
                </div>
                <div class="contract-cost">
                    Cost is @item.Cost
                </div>
                <div class="contract-cost">
                    Landlord Name is @item.LandLordName
                </div>
                <div class="contract-cost">
                    Landlord Surname is @item.LandLordSurName
                </div>
                <div class="contract-cost">
                    Lessee Name is @item.LesseeName
                </div>
                <div class="contract-cost">
                    Lessee SurName is @item.LesseeSurName
                </div>
            </div>
        }
    </div>
}
